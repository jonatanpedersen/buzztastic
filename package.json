{
  "name": "buzztastic",
  "version": "1.0.0",
  "description": "",
  "main": "build/server",
  "scripts": {
    "build": "concurrently \"npm run tsc\" \"npm run webpack\"",
    "build-watch": "concurrently \"npm run tsc-watch\" \"npm run webpack-watch\"",
    "develop": "concurrently \"npm run build-watch\" \"npm run nodemon\"",
    "tsc": "cross-env NODE_ENV=production tsc",
    "tsc-watch": "cross-env NODE_ENV=development tsc --watch",
    "start": "node --harmony_async_iteration --experimental-modules .",
    "nodemon": "nodemon --watch build --harmony_async_iteration --experimental-modules",
    "test": "mocha --harmony_async_iteration",
    "webpack": "cross-env NODE_ENV=production parallel-webpack",
    "webpack-watch": "cross-env NODE_ENV=development parallel-webpack --watch --no-stats"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonatanpedersen/buzztastic.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jonatanpedersen/buzztastic/issues"
  },
  "homepage": "https://github.com/jonatanpedersen/buzztastic#readme",
  "dependencies": {
    "@jambon/core": "1.2.3",
    "@jambon/cors": "1.2.3",
    "@jambon/json": "1.2.3",
    "@jambon/pug": "1.2.3",
    "@jambon/router": "1.2.3",
    "@jambon/static": "1.2.3",
    "amqplib": "0.5.2",
    "body-parser": "1.18.2",
    "compression": "1.7.2",
    "express": "4.16.3",
    "mongodb": "2.2.35",
    "pug": "2.0.3",
    "react-router-dom": "4.2.2",
    "shortid": "2.2.8",
    "socket.io": "2.0.4",
    "typescript": "2.7.2",
    "uuid": "3.2.1",
    "vhost": "3.0.2"
  },
  "devDependencies": {
    "@std/esm": "0.25.3",
    "@types/chai": "4.1.2",
    "@types/mocha": "2.2.48",
    "@types/mongodb": "2.2.20",
    "@types/node": "8.9.5",
    "@types/react": "^16.0.41",
    "@types/socket.io": "1.4.32",
    "babel-core": "6.26.0",
    "babel-loader": "7.1.4",
